package rules.manager

import burnoutrulesengine.burnoutrulesengine.model.WeeklyRecordFact

global java.util.Map symptomStats

rule "MAN_Q_Burnout_Symptom_Combinations"
when
    $f : WeeklyRecordFact(hasFatigueAndConcentrationLoss == true)
then
    Integer count = (Integer) symptomStats.getOrDefault("Umor+Pad koncentracije", 0);
    symptomStats.put("Umor+Pad koncentracije", count + 1);
    System.out.println("ðŸ§  Evidentirana kombinacija simptoma: Umor+Pad koncentracije");
end
